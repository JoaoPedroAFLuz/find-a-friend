    "@vitest/coverage-v8": "0.34.6",
    "@vitest/ui": "0.34.6",
    "eslint": "8.54.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-prettier": "5.0.1",
    "prettier": "3.1.0",
    "prisma": "5.6.0",
    "tsup": "8.0.1",
    "tsx": "4.5.0",
    "typescript": "5.3.2",
    "vite-tsconfig-paths": "4.2.1",
    "vitest": "0.34.6"
  },
